{
  "manifest_version": 2,
  "name": "URL Saver",
  "version": "1.0",
  "description": "Save visited URLs to a database",
  "permissions": ["activeTab", "webNavigation", "tabs", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "icons": {
    "16": "images/icon16.png"
  }
}
